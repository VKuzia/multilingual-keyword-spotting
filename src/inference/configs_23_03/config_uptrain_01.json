{
  "data": {
    "root": "",
    "table": "csv/all_c_pacanami.csv",
    "batch_size": 16,
    "is_wav": true,
    "cuda": true
  },
  "model": {
    "embedding": {
      "path": "all_train/final/embedding.pth",
      "name": "cnn_y",
      "output": 1024,
      "freeze": true,
      "size": 4
    },
    "head_pattern": {
      "name": "softmax",
      "input": 1024,
      "output": 2
    }
  },
  "targets": [
    "боньк",
    "братан",
    "вжух",
    "дыщ",
    "лево",
    "опа",
    "право",
    "прыг",
    "телеграм"
  ],
  "optimizer": {
    "name": "Adam",
    "lr": 0.0001
  },
  "scheduler": {
    "name": "exponential",
    "gamma": 0.8
  },
  "loss": {
    "name": "xent"
  },
  "epochs": 10,
  "batches_per_validation": 200,
  "batches_per_epoch": 1000,
  "save_after": 2
}